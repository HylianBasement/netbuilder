{
  "name": "@rbxts/netbuilder",
  "version": "0.2.2",
  "description": "Abstract and declarative networking library for Roblox",
  "main": "out/init.lua",
  "scripts": {
    "build": "rbxtsc --verbose",
    "build:example": "rbxtsc --verbose --type=game -i ./include -p ./example",
    "serve:example": "rojo serve ./example/default.project.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "roblox-ts",
    "Roblox",
    "Networking",
    "Event"
  ],
  "author": "Rimuy",
  "license": "MIT",
  "types": "out/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rimuy/netbuilder.git"
  },
  "bugs": {
    "url": "https://github.com/Rimuy/netbuilder/issues"
  },
  "files": [
    "out",
    "!out/test.lua",
    "!out/example",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^1.3.3-types.1",
    "@rbxts/types": "^1.0.573",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-roblox-ts": "^0.0.32",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@rbxts/rust-classes": "^0.11.1"
  }
}
